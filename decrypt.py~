import sys

def decrypt(fcyphertext,fsample):
    
    c = open(fcyphertext,'r')
    s = open(fsample, 'r')

    cyphertext = c.read()
    sample = s.read()

    cyphertext_freq = dict()
    sample_freq = dict()
    
    alphabet = "qwertyuiopasdfghjklmnbvcxz"

    for a in alphabet:
    	cyphertext_freq[a] = 0
    	sample_freq[a] = 0


    for letter in cyphertext:
    	if (97 <= ord(letter.lower()) < 123):
    		cyphertext_freq[letter.lower()] += 1

    for letter in sample:
    	if (97 <= ord(letter.lower()) < 123):
    		sample_freq[letter.lower()] += 1

 	c_max = {
 		"value" : 0,
 		"key" : None
 	}

 	c_min = {
 		"value" : 9999999,
 		"key" : None
 	}

 	for k,v in cyphertext_freq.items():
 		if(c_max["value"] < v):
 			c_max["value"] = v
 			c_max["key"] = k
 		if(c_min["value"] > v):
 			c_min["value"] = v
 			c_min["key"] = k

	s_max = {
 		"value" : 0,
 		"key" : None
 	}
 	
 	s_min = {
 		"value" : 9999999,
 		"key" : None
 	}

 	for k,v in sample_freq.items():
 		if(s_max["value"] < v):
 			s_max["value"] = v
 			s_max["key"] = k

 		if(s_min["value"] > v):
 			s_min["value"] = v
 			s_min["key"] = k
 	print c_max, c_min
  

decrypt(sys.argv[1], sys.argv[2])